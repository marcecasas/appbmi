<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
    xmlns:gap   = "http://phonegap.com/ns/1.0"
    id          = "com.phonegap.example"
    versionCode = "10"
    version     = "1.0.0" >

<!-- versionCode is optional and Android only -->

  <name>PhoneGap Example</name>

  <description>
      An example for phonegap build docs.
  </description>

  <author email="casas.marcelo@canossamilano.org">
      Marcelo Casas
  </author>

<platform name="android" />
<icon src="icona2.png" platform="android" qualifier="ldpi" />
<icon src="icona2.png" platform="android" qualifier="mdpi" />
<icon src="icona2.png" platform="android" qualifier="hdpi" />

<access origin="+" />
<plugin name="cordova-plugin-whitelist" source="npm" />
<allow-navigation href="*;//*getskeleton.com/*" />
</widget>

